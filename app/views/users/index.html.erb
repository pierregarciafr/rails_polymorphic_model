<h1>Users index</h1>
<p>Nb of users</p>
<%= @users.count %>
<p>List of users</p>
<table>
  <thead>
    <tr>
      <th>Pseudo</th>
      <th>Email</th>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <th><%= user.member.pseudo %></th>
      <th><%= user.member.email %></th>
      <th><%= user.name %></th>
      <th></th>
      <th><%= link_to 'Show', user_path(user) %></th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
<% end %>
</tbody>
</table>
